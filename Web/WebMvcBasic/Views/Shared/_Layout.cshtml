<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css")
    @Styles.Render("https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap-theme.min.css")
    @Scripts.Render("https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js")
</head>
<body>
    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
